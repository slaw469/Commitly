{
  "buildCommand": "cd ../.. && pnpm install && pnpm --filter commitly-web build",
  "outputDirectory": "dist",
  "installCommand": "echo 'Using buildCommand for install'",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
